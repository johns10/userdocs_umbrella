<%= f = form_for @changeset, "#",
  id: @id,
  phx_target: @myself.cid,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= Layout.select_input(f, :team_id, @select_lists.teams, [
    selected: @parent_id || "",
    id: @field_ids.team_id
  ]) %>

  <%= Layout.text_input(f, :name, [
    id: @field_ids.name
  ]) %>

  <%= Layout.text_input(f, :base_url, [
    id: @field_ids.base_url
  ]) %>

  <%= submit "Save", phx_disable_with: "Saving...", class: "button is-link" %>

</form>
