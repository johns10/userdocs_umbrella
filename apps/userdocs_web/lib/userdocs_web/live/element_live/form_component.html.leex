<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "element-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :name %>
  <%= text_input f, :name %>
  <%= error_tag f, :name %>

  <%= label f, :strategy %>
  <%= text_input f, :strategy %>
  <%= error_tag f, :strategy %>

  <%= label f, :selector %>
  <%= text_input f, :selector %>
  <%= error_tag f, :selector %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
