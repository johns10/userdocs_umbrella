<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "user-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :email %>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= text_input f, :password %>
  <%= error_tag f, :password %>

  <%= label f, :default_team_id %>
  <%= number_input f, :default_team_id %>
  <%= error_tag f, :default_team_id %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
