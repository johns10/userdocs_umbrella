<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "content-form",
  phx_target: @myself.cid,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :team_id %>
  <%= select f, :team_id, @teams_select_options, value: @current_user.default_team_id %>
  <%= error_tag f, :team_id %>

  <%= label f, :name %>
  <%= text_input f, :name %>
  <%= error_tag f, :name %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
